@model FrontEnd.API.Models.BoleteriaReservados

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Deseas eliminar este registro?</h3>
<div>
    <h4>BoleteriaReservados</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cantidad)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cantidad)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CodBoleteriaNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CodBoleteriaNavigation.Descripcion)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CodTicketsNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CodTicketsNavigation.Nreserva)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="CodBoletaReservado" />
        <input type="hidden" asp-for="CodBoleteria" />
        <input type="hidden" asp-for="CodTickets" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Regresar</a>
    </form>
</div>
